    <title>Sankey Diagram</title>
    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
</head>
<body>
    <h1>Sankey Diagram of MRICloud Data</h1>
    <div id="sankey"></div>
    
    <script>
        // Load the Sankey diagram HTML file
        Plotly.d3.html("sankey_diagram.html", function(err, data) {
            if (err) throw err;
            document.getElementById('sankey').innerHTML = data;
        });
    </script>
</body>
</html>
